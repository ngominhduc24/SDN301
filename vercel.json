{
    "version": 2,
    "builds": [
      {
        "src": "src/bin/www",
        "use": "@vercel/node"
      },
      {
        "src": "public/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/((?!.*\\.\\w+$).*)",
        "dest": "/src/bin/www"
      },
      {
        "src": "/(.+\\.[a-z]+)$",
        "dest": "/public/$1"
      }
    ],
    "build": {
      "command": "npm run build"
    }
  }
  